{
    "name": "ini-for-vscode",
    "displayName": "Ini for VSCode",
    "description": "Provide outline view and section folding for INI file.",
    "version": "0.0.4",
    "publisher": "DavidWang",
    "homepage": "https://github.com/davidwang88/ini-for-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/davidwang88/ini-for-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/davidwang88/ini-for-vscode/issues"
	},
	"license": "GPL",
    "engines": {
        "vscode": "^1.25.0"
    },
    "keywords": [
		"ini",
		"reg",
		"cfg",
		"outline",
		"folding"
	],
    "categories": [
        "Other"
    ],
    "icon": "icon.png",
    "activationEvents": [
        "onLanguage:ini"
    ],
    "main": "./src/extension",
    "contributes": {
        "snippets": [
            {
                "language": "ini",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
